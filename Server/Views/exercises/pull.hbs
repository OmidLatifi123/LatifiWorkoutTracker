<h1> Pull Page </h1>

<a href="/exercises/createPull"> Create Pull</a>

<ul class="card-body center">
    {{#each pull}}
 <li class="card m-4 text-center col-lg-4">
            <p>{{this.reps}}</p>       
            <h6>{{this.title}}</h6>
            <p>{{{this.description}}}</p>
   

   <a href="/exercises/editPull/{{this._id}}" class="btn btn-primary">
                Edit
            </a>
            <a href="/exercises/delete/{{this._id}}" class="btn btn-primary">
                Delete
            </a>
    </li> 


    {{/each}}
</ul>